            self.wfile.write(json.dumps(response, indent=2).encode('utf-8'))
            
        except Exception as e:
            print(f"Error en psychological state: {e}")
            self.send_error(500, f"Error interno: {str(e)}")

    def serve_quantum_metrics(self):
