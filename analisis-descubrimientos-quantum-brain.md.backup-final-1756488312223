    const [spotData, futuresData, optionsData] = await Promise.all([
        getSpotData(),
        getFuturesData(),
        getOptionsData()
    ]);
    
    return {
        spot: spotData,      // 602 s√≠mbolos
        futures: futuresData, // 475 s√≠mbolos  
