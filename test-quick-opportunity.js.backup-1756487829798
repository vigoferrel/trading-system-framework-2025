/**
 * ğŸš€ Test RÃ¡pido del Opportunity Master System
 * VerificaciÃ³n rÃ¡pida de funcionalidad bÃ¡sica
 */

const axios = require('axios');

const OPPORTUNITY_MASTER_URL = 'http://localhost:4601';
const FRONTEND_URL = 'http://localhost:4603';

async function quickTest() {
    console.log('ğŸš€ Test RÃ¡pido del Opportunity Master System...\n');
    
    try {
        // Test 1: Health Check
        console.log('ğŸ” Test 1: Health Check del Opportunity Master');
        const healthResponse = await axios.get(`${OPPORTUNITY_MASTER_URL}/health`, { timeout: 5000 });
        console.log(`âœ… Health Check: ${healthResponse.data.status}`);
        console.log(`ğŸ“Š Arquitectura: ${healthResponse.data.architecture}\n`);
        
        // Test 2: Frontend Status
        console.log('ğŸ” Test 2: Status del Frontend');
        const frontendResponse = await axios.get(`${FRONTEND_URL}/api/status`, { timeout: 5000 });
        console.log(`âœ… Frontend Status: ${frontendResponse.data.status}`);
        console.log(`ğŸ“Š Conectado: ${frontendResponse.data.connected}\n`);
        
        // Test 3: Oportunidades (con timeout mÃ¡s largo)
        console.log('ğŸ” Test 3: API de Oportunidades (puede tardar...)');
        const opportunitiesResponse = await axios.get(`${OPPORTUNITY_MASTER_URL}/api/opportunities`, { timeout: 15000 });
        
        if (opportunitiesResponse.data.success) {
            const data = opportunitiesResponse.data.data;
            console.log(`âœ… Oportunidades generadas: ${data.top ? data.top.length : 0}`);
            console.log(`ğŸ“Š Sentimiento del mercado: ${data.market ? data.market.sentiment : 'N/A'}`);
            console.log(`ğŸ¯ SeÃ±ales fuertes: ${data.market ? data.market.strongSignals : 0}`);
            
            if (data.top && data.top.length > 0) {
                console.log(`ğŸ† Top 3 oportunidades:`);
                data.top.slice(0, 3).forEach((op, index) => {
                    console.log(`   ${index + 1}. ${op.symbol}: ${op.score} (${op.action})`);
                });
            }
            
            if (data.quantum) {
                console.log(`ğŸŒ™ MÃ©tricas CuÃ¡nticas:`);
                console.log(`   - Coherence: ${(data.quantum.coherence * 100).toFixed(1)}%`);
                console.log(`   - Consciousness: ${(data.quantum.consciousness * 100).toFixed(1)}%`);
                console.log(`   - Entanglement: ${(data.quantum.entanglement * 100).toFixed(1)}%`);
            }
        } else {
            console.log('âŒ Error en oportunidades');
        }
        
        console.log('\nğŸ‰ Test rÃ¡pido completado exitosamente!');
        console.log('ğŸŒ™ Opportunity Master System funcionando correctamente');
        console.log('ğŸ† Arquitectura SPOT â†’ OPTIONS â†’ FUTURES verificada');
        
    } catch (error) {
        console.error('âŒ Error en test rÃ¡pido:', error.message);
        
        if (error.code === 'ECONNREFUSED') {
            console.log('ğŸ’¡ AsegÃºrate de que ambos sistemas estÃ©n ejecutÃ¡ndose:');
            console.log('   - Opportunity Master: node opportunity-master-system.js');
            console.log('   - Frontend: node frontend-opportunity-master.js');
        }
    }
}

// Ejecutar test
quickTest();
