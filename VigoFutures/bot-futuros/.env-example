# Configuración de entorno para Bot de Futuros QBTC
# Sistema Tandalones - Independiente

# Configuración de Binance API
BINANCE_API_KEY=tu_api_key_de_binance_aqui
BINANCE_API_SECRET=tu_api_secret_de_binance_aqui
BINANCE_TESTNET=true

# Configuración del servidor
SERVER_PORT=5500
SERVER_HOST=localhost

# Configuración de trading
TRADING_PAIRS=BTCUSDT,ETHUSDT,ADAUSDT,SOLUSDT,DOGEUSDT
TRADING_LEVERAGE=20
TRADING_MARGIN_TYPE=ISOLATED
TRADING_STOP_LOSS_PERCENT=2.5
TRADING_TAKE_PROFIT_PERCENT=5.0
TRADING_MAX_POSITION_SIZE=100
TRADING_MIN_POSITION_SIZE=10

# Configuración cuántica
QUANTUM_MIN_EDGE=0.0025
QUANTUM_MIN_CONFIDENCE=0.75
QUANTUM_MAX_RISK_PER_TRADE=0.02
QUANTUM_RISK_REWARD_RATIO=2.5
QUANTUM_LEVERAGE_MULTIPLIER=1.618
QUANTUM_ZURITA_MULTIPLIER=7919
QUANTUM_GRAVITATIONAL_LENSING_FACTOR=1.333
QUANTUM_TEMPORAL_ADVANTAGE=0.001

# Configuración Feynman
FEYNMAN_COMPLEX_REAL=9
FEYNMAN_COMPLEX_IMAGINARY=16
FEYNMAN_LAMBDA_FREQUENCY=888
FEYNMAN_LOG_PRIME_FACTOR=8.977

# Configuración de logging
LOGGING_LEVEL=info
LOGGING_ENABLE_CONSOLE=true
LOGGING_ENABLE_FILE=true
LOGGING_MAX_FILE_SIZE=10MB
LOGGING_MAX_FILES=5

# Configuración de la base de datos
DATABASE_TYPE=sqlite
DATABASE_ENABLE_WAL=true

# Configuración de monitoreo
MONITORING_ENABLE_METRICS=true
MONITORING_METRICS_INTERVAL=60000
MONITORING_ENABLE_HEALTH_CHECK=true
MONITORING_HEALTH_CHECK_INTERVAL=30000