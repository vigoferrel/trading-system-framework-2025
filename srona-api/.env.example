# SRONA API Configuration
NODE_ENV=development
PORT=3002
LOG_LEVEL=info

# CORS Configuration
ALLOWED_ORIGINS=https://yourdomain.com,https://api.yourdomain.com

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=1000

# Quantum System Configuration
QUANTUM_UPDATE_INTERVAL=30000
QUANTUM_COHERENCE_THRESHOLD=0.888
QUANTUM_ENABLE_REAL_COMPUTING=true
QUANTUM_ENABLE_INTEGRATION=true
QUANTUM_ENABLE_ADVANCED_ALGORITHMS=true
QUANTUM_ENABLE_CONSCIOUSNESS=true
QUANTUM_ENABLE_INFINITE_PROFIT_PLANE=true
QUANTUM_MAX_OPERATIONS=1000
QUANTUM_ENABLE_CACHE=true

# Edge System Configuration
EDGE_UPDATE_INTERVAL=1000
EDGE_MIN_THRESHOLD=0.001
EDGE_MAX_POSITION_SIZE=10000
EDGE_RISK_TOLERANCE=0.02
EDGE_QUANTUM_PRECISION=1e-12

# Integration System Configuration
INTEGRATION_INTERVAL=60000
INTEGRATION_COHERENCE_THRESHOLD=0.888
INTEGRATION_ENABLE_ADVANCED_FEATURES=true

# Security Configuration
HELMET_CSP_ENABLED=true
COMPRESSION_LEVEL=6
COMPRESSION_THRESHOLD=1024

# Logging Configuration
LOG_FILE_ERROR=logs/srona-error.log
LOG_FILE_COMBINED=logs/srona-combined.log
LOG_MAX_SIZE=20m
LOG_MAX_FILES=14d

# Cache Configuration
CACHE_DEFAULT_TTL=60000
CACHE_MAX_SIZE=1000
CACHE_CLEANUP_INTERVAL=300000

# API Keys (Add your actual keys here)
BINANCE_API_KEY=your_binance_api_key_here
BINANCE_SECRET_KEY=your_binance_secret_key_here
BINANCE_TESTNET=true

# Database Configuration (if needed)
DATABASE_URL=mongodb://localhost:27017/srona
REDIS_URL=redis://localhost:6379

# Monitoring and Analytics
ENABLE_PERFORMANCE_MONITORING=true
ENABLE_ERROR_TRACKING=true
ANALYTICS_API_KEY=your_analytics_key_here

# WebSocket Configuration
WS_HEARTBEAT_INTERVAL=30000
WS_MAX_CONNECTIONS=1000
WS_ENABLE_COMPRESSION=true