            showSuccess(message) {
                const successDiv = document.createElement('div');
                successDiv.className = 'success';
                successDiv.textContent = message;
                document.querySelector('.container').insertBefore(successDiv, document.querySelector('.quantum-header'));
                
                setTimeout(() => {
                    successDiv.remove();
                }, 5000);
            }
        }

