/**
 * Test de Errores del Frontend
 * Verifica que los conflictos y errores se han resuelto
 */

console.log('ðŸ” Iniciando Test de Errores del Frontend...');

// FunciÃ³n para verificar que no hay conflictos de variables
function checkVariableConflicts() {
    console.log('ðŸ” Verificando conflictos de variables...');
    
    // Verificar que systemState no estÃ¡ duplicado
    if (typeof systemState !== 'undefined' && typeof mlSystemState !== 'undefined') {
        console.log('âœ… Variables systemState y mlSystemState coexisten correctamente');
    } else if (typeof systemState !== 'undefined') {
        console.log('âœ… systemState disponible (script.js)');
    } else if (typeof mlSystemState !== 'undefined') {
        console.log('âœ… mlSystemState disponible (ml-dashboard.js)');
    } else {
        console.log('âš ï¸ Ninguna variable systemState encontrada');
    }
    
    // Verificar configuraciÃ³n centralizada
    if (typeof window.CONFIG !== 'undefined') {
        console.log('âœ… ConfiguraciÃ³n centralizada cargada');
        console.log('   - Frontend URL:', window.CONFIG.FRONTEND_URL);
        console.log('   - Core URL:', window.CONFIG.CORE_URL);
    } else {
        console.log('âŒ ConfiguraciÃ³n centralizada no encontrada');
    }
    
    // Verificar API_UTILS
    if (typeof window.API_UTILS !== 'undefined') {
        console.log('âœ… API_UTILS disponible');
    } else {
        console.log('âŒ API_UTILS no encontrado');
    }
}

// FunciÃ³n para verificar elementos del DOM
function checkDOMElements() {
    console.log('ðŸ” Verificando elementos del DOM...');
    
    const criticalElements = [
        'connectionStatus',
        'lastUpdate',
        'marketGrid',
        'matrixGrid',
        'signalsList',
        'alertsContainer',
        'projectionsContainer',
        'tradeModeBadge'
    ];
    
    let foundElements = 0;
    
    criticalElements.forEach(id => {
        const element = document.getElementById(id);
        if (element) {
            console.log(`âœ… ${id}: Encontrado`);
            foundElements++;
        } else {
            console.log(`âŒ ${id}: No encontrado`);
        }
    });
    
    console.log(`ðŸ“Š Elementos encontrados: ${foundElements}/${criticalElements.length}`);
    
    if (foundElements === criticalElements.length) {
        console.log('ðŸŽ‰ Todos los elementos crÃ­ticos del DOM estÃ¡n presentes');
    } else {
        console.log('âš ï¸ Algunos elementos del DOM faltan');
    }
}

// FunciÃ³n para verificar funciones crÃ­ticas
function checkCriticalFunctions() {
    console.log('ðŸ” Verificando funciones crÃ­ticas...');
    
    const criticalFunctions = [
        'updateConnectionStatus',
        'initializeDOMElements',
        'checkConnection',
        'loadAllData'
    ];
    
    let foundFunctions = 0;
    
    criticalFunctions.forEach(funcName => {
        if (typeof window[funcName] === 'function') {
            console.log(`âœ… ${funcName}: Disponible`);
            foundFunctions++;
        } else {
            console.log(`âŒ ${funcName}: No disponible`);
        }
    });
    
    console.log(`ðŸ“Š Funciones encontradas: ${foundFunctions}/${criticalFunctions.length}`);
}

// FunciÃ³n para verificar conectividad
async function checkConnectivity() {
    console.log('ðŸ” Verificando conectividad...');
    
    try {
        // Verificar frontend
        const frontendResponse = await fetch('http://localhost:4603/health');
        if (frontendResponse.ok) {
            console.log('âœ… Frontend Server: Conectado');
        } else {
            console.log('âŒ Frontend Server: Error de respuesta');
        }
    } catch (error) {
        console.log('âŒ Frontend Server: No conectado');
    }
    
    try {
        // Verificar core
        const coreResponse = await fetch('http://localhost:4601/health');
        if (coreResponse.ok) {
            console.log('âœ… Core System: Conectado');
        } else {
            console.log('âŒ Core System: Error de respuesta');
        }
    } catch (error) {
        console.log('âŒ Core System: No conectado');
    }
}

// FunciÃ³n para verificar errores en la consola
function checkConsoleErrors() {
    console.log('ðŸ” Verificando errores en consola...');
    
    // Capturar errores futuros
    const originalError = console.error;
    const originalWarn = console.warn;
    
    let errorCount = 0;
    let warningCount = 0;
    
    console.error = function(...args) {
        errorCount++;
        originalError.apply(console, args);
    };
    
    console.warn = function(...args) {
        warningCount++;
        originalWarn.apply(console, args);
    };
    
    // Restaurar despuÃ©s de 5 segundos
    setTimeout(() => {
        console.error = originalError;
        console.warn = originalWarn;
        
        console.log(`ðŸ“Š Errores capturados: ${errorCount}`);
        console.log(`ðŸ“Š Advertencias capturadas: ${warningCount}`);
        
        if (errorCount === 0) {
            console.log('ðŸŽ‰ No se detectaron errores en la consola');
        } else {
            console.log('âš ï¸ Se detectaron errores en la consola');
        }
    }, 5000);
}

// FunciÃ³n principal de test
async function runFrontendErrorTest() {
    console.log('ðŸŒ™ Iniciando Test Completo de Errores del Frontend...\n');
    
    // Esperar a que el DOM estÃ© listo
    if (document.readyState === 'loading') {
        await new Promise(resolve => {
            document.addEventListener('DOMContentLoaded', resolve);
        });
    }
    
    // Ejecutar verificaciones
    checkVariableConflicts();
    console.log('');
    
    checkDOMElements();
    console.log('');
    
    checkCriticalFunctions();
    console.log('');
    
    await checkConnectivity();
    console.log('');
    
    checkConsoleErrors();
    console.log('');
    
    console.log('ðŸŒ™ Test de Errores del Frontend completado');
    console.log('ðŸ“‹ Resumen:');
    console.log('   - Verificar que no hay conflictos de variables');
    console.log('   - Verificar que todos los elementos del DOM estÃ¡n presentes');
    console.log('   - Verificar que las funciones crÃ­ticas estÃ¡n disponibles');
    console.log('   - Verificar conectividad con servidores');
    console.log('   - Monitorear errores en consola por 5 segundos');
}

// Ejecutar test
runFrontendErrorTest().catch(console.error);
