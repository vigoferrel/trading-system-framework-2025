                    print(f"   ğŸ“ˆ Profit inverso: ${ir['inverse_profit_usd']:.2f}")
                    print(f"   ğŸ’ Profit prima enhanced: ${ir['prime_enhanced_profit_usd']:.2f}")
                    print(f"   ğŸ’° Profit neto prima: ${ir['prime_net_profit_usd']:.2f}")
                    print(f"   â° Horizonte cuÃ¡ntico: {ir['time_horizon_min']} min")
            
            print(f"\nğŸš€ ESTADO: {data['prime_recommended_action']}")
            print(f"âœ… Listo para ejecuciÃ³n prima: {data['prime_ready_for_execution']}")
            
        else:
            print(f"\nâŒ {report}")
    
    asyncio.run(main())
