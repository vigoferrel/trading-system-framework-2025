{
  "name": "trading-system-framework-2025",
  "version": "3.0.0-alpha",
  "private": true,
  "description": "QBTC - Professional Algorithmic Trading Framework with Deterministic RNG and Advanced Risk Management",
  "main": "src/launch-qbtc-system.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "algorithmic-trading",
    "cryptocurrency",
    "binance",
    "trading-bot",
    "risk-management",
    "deterministic",
    "options-trading",
    "futures-trading"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vigoferrel/trading-system-framework-2025.git"
  },
  "bugs": {
    "url": "https://github.com/vigoferrel/trading-system-framework-2025/issues"
  },
  "homepage": "https://github.com/vigoferrel/trading-system-framework-2025#readme",
  "scripts": {
    "start": "node src/launch-qbtc-system.js",
    "dev": "nodemon src/launch-qbtc-system.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:performance": "jest --testPathPattern=tests/performance",
    "test:system": "node test-system.js",
    "test:ci": "CI=true jest --ci --coverage --watchAll=false",
    "test:complete": "node scripts/run-complete-pipeline.js",
    "test:simple": "npx jest --testMatch='**/__tests__/unit/*-simple.test.js'",
    "test:regression": "npx jest --testMatch='**/__tests__/regression/**/*.test.js'",
    "test:binance": "node scripts/test-binance-connection.js",
    "coverage": "jest --coverage",
    "lint": "eslint src/ tests/ --ext .js",
    "lint:fix": "eslint src/ tests/ --ext .js --fix",
    "lint:quantum": "npx eslint . --ext .js --max-warnings=0",
    "audit:security": "npm audit --audit-level high",
    "audit:fix": "npm audit fix",
    "benchmark": "node benchmark/performance-tests.js",
    "rng:quality": "node -e \"const {qualityTest} = require('./src/utils/kernel-rng.js'); console.log(JSON.stringify(qualityTest(100000), null, 2));\"",
    "demo": "node demo/realistic-prices-demo.js",
    "demo:test": "node demo/realistic-prices-demo.js test",
    "demo:vol": "node demo/realistic-prices-demo.js vol",
    "dashboard": "node src/dashboard/central-control-dashboard.js",
    "monitor": "node src/monitoring/intelligent-monitor.js",
    "optimize": "node src/optimization/gamma-hedging-engine.js",
    "yield": "node src/yield/yield-strategy-engine.js",
    "paper-trade": "NODE_ENV=paper node src/launch-qbtc-system.js",
    "live-trade": "NODE_ENV=production node src/launch-qbtc-system.js",
    "validate": "npm run test:system && npm run rng:quality && npm run lint",
    "precommit": "npm run lint:fix && npm run test && npm run test:system",
    "build:docs": "jsdoc -c jsdoc.config.json",
    "clean": "rm -rf coverage/ docs/ .nyc_output/",
    "reports:generate": "node scripts/generate-reports.js",
    "quality:monitor": "node scripts/quality-monitor.js",
    "quality:dashboard": "node scripts/quality-monitor.js && echo Dashboard disponible en __tests__/reports/quality-dashboard.md",
    "unified:core": "node src/core/reverse-engineering-core.js",
    "unified:system": "node scripts/run-integrated-system.js",
    "unified:dashboard": "node src/dashboard/start-dashboard.js",
    "background:start": "node scripts/start-background.js",
    "background:stop": "node scripts/stop-background.js",
    "background:status": "node scripts/check-status.js",
    "background:logs": "node scripts/check-status.js --logs --lines 20"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "binance-api-node": "^0.12.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "node-fetch": "^3.3.2",
    "path-to-regexp": "^6.3.0",
    "router": "^1.3.8",
    "socket.io": "^4.8.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "nock": "^13.4.0",
    "sinon": "^17.0.1",
    "nyc": "^15.1.0",
    "@eslint/js": "^9.35.0"
  }
}
