/**
 * 🧠 TEST REVOLUCIONARIO: PROYECCIÓN NEURONAL CON TAKE PROFIT CUÁNTICO
 * 🔮 SISTEMA DE NEURONAS ESPECIALIZADAS EN PREDICCIÓN DE PRECIOS
 * ⚛️ TAKE PROFIT DINÁMICO BASADO EN FIBONACCI + PRIMOS + LEONARDO
 */

const axios = require('axios');

const API_BASE_URL = 'http://localhost:4601';

// 🎯 ESCENARIOS DE PRUEBA REVOLUCIONARIOS
const testScenarios = [
    {
        name: '🚀 BTCUSDT - LEVERAGE 75x - HORIZONTE 30D',
        symbol: 'BTCUSDT',
        currentPrice: 45000,
        timeHorizon: '30d',
        leverage: 75
    },
    {
        name: '⚡ ETHUSDT - LEVERAGE 50x - HORIZONTE 7D',
        symbol: 'ETHUSDT',
        currentPrice: 2800,
        timeHorizon: '7d',
        leverage: 50
    },
    {
        name: '🌙 ADAUSDT - LEVERAGE 100x - HORIZONTE 1D',
        symbol: 'ADAUSDT',
        currentPrice: 0.45,
        timeHorizon: '1d',
        leverage: 100
    },
    {
        name: '🔥 SOLUSDT - LEVERAGE 125x - HORIZONTE 4H',
        symbol: 'SOLUSDT',
        currentPrice: 95,
        timeHorizon: '4h',
        leverage: 125
    }
];

// 🧠 FUNCIÓN PRINCIPAL DE PRUEBA
async function testNeuralProjectionQuantum() {
    console.log('🧠 ===========================================');
    console.log('🔮 PROYECCIÓN NEURONAL CON TAKE PROFIT CUÁNTICO');
    console.log('⚛️ SISTEMA REVOLUCIONARIO DE PREDICCIÓN');
    console.log('🎯 NEURONAS ESPECIALIZADAS EN ACCIÓN');
    console.log('🧠 ===========================================\n');

    for (const scenario of testScenarios) {
        console.log(`\n🎯 ${scenario.name}`);
        console.log('─'.repeat(60));
        
        try {
            const response = await axios.post(`${API_BASE_URL}/api/neural-price-projection`, {
                symbol: scenario.symbol,
                currentPrice: scenario.currentPrice,
                timeHorizon: scenario.timeHorizon,
                leverage: scenario.leverage
            });

            if (response.data.success) {
                displayProjectionResults(response.data.data, scenario);
            } else {
                console.log(`❌ Error en proyección: ${response.data.error}`);
            }
        } catch (error) {
            console.log(`❌ Error de conexión: ${error.message}`);
        }
        
        console.log('\n' + '─'.repeat(60));
    }
}

// 🎯 FUNCIÓN PARA MOSTRAR RESULTADOS REVOLUCIONARIOS
function displayProjectionResults(projection, scenario) {
    const { symbol, current_price, leverage_applied, unified_projection, take_profit_levels, timeframe_projections, neural_breakdown, risk_assessment } = projection;

    // 📊 PROYECCIÓN UNIFICADA
    console.log(`\n📊 PROYECCIÓN UNIFICADA NEURONAL:`);
    console.log(`   🎯 Precio Objetivo: $${unified_projection.target_price.toFixed(2)}`);
    console.log(`   📈 Cambio Esperado: ${((unified_projection.target_price - current_price) / current_price * 100).toFixed(2)}%`);
    console.log(`   🧠 Confianza: ${unified_projection.confidence}`);
    console.log(`   ⚛️ Probabilidad de Éxito: ${unified_projection.probability_success}`);
    console.log(`   🎭 Consenso Neural: ${unified_projection.neural_consensus}`);
    console.log(`   ⏰ Timeframe Esperado: ${unified_projection.expected_timeframe}`);

    // 🎯 TAKE PROFIT LEVELS REVOLUCIONARIOS
    console.log(`\n🎯 TAKE PROFIT LEVELS CUÁNTICOS (${leverage_applied}x leverage):`);
    take_profit_levels.slice(0, 5).forEach((tp, index) => {
        console.log(`   ${index + 1}. ${tp.zone_name} (${tp.type})`);
        console.log(`      💰 Precio: $${tp.price.toFixed(2)}`);
        console.log(`      🚀 Ganancia: ${tp.gain_percent}`);
        console.log(`      🎯 Confianza: ${(tp.confidence * 100).toFixed(1)}%`);
    });

    // 🧠 ANÁLISIS NEURONAL DETALLADO
    console.log(`\n🧠 ANÁLISIS NEURONAL ESPECIALIZADO:`);
    Object.entries(neural_breakdown).forEach(([neuron, data]) => {
        const neuronName = neuron.replace(/_/g, ' ').toUpperCase();
        const priceChange = ((data.target_price - current_price) / current_price * 100).toFixed(2);
        console.log(`   🧠 ${neuronName}:`);
        console.log(`      💰 Precio: $${data.target_price.toFixed(2)} (${priceChange}%)`);
        console.log(`      🎯 Confianza: ${(data.confidence * 100).toFixed(1)}%`);
        console.log(`      ⏰ Días: ${data.expected_days || 'N/A'}`);
    });

    // 📈 PROYECCIONES POR TIMEFRAME
    console.log(`\n📈 PROYECCIONES MULTI-TIMEFRAME:`);
    Object.entries(timeframe_projections).forEach(([tfName, tfData]) => {
        if (tfData.relevant_tp_levels.length > 0) {
            console.log(`   ⏰ ${tfName.toUpperCase()} (${tfData.timeframe}):`);
            console.log(`      💰 Precio: $${tfData.projected_price.toFixed(2)} (${tfData.price_change_percent})`);
            console.log(`      🎯 TP Relevantes: ${tfData.relevant_tp_levels.length}`);
            console.log(`      🔢 Ciclo Fibonacci: ${tfData.fibonacci_cycle}`);
            console.log(`      🔢 Resonancia Prima: ${tfData.prime_resonance}`);
        }
    });

    // ⚠️ ANÁLISIS DE RIESGO
    console.log(`\n⚠️ ANÁLISIS DE RIESGO CUÁNTICO:`);
    console.log(`   📉 Prob. Max Drawdown: ${risk_assessment.max_drawdown_probability}`);
    console.log(`   🛑 Prob. Stop Loss: ${risk_assessment.stop_loss_trigger_probability}`);
    console.log(`   ✅ Prob. Take Profit: ${risk_assessment.take_profit_achievement_probability}`);
    console.log(`   ⚛️ Factor Volatilidad: ${risk_assessment.neural_volatility_factor.toFixed(2)}`);
    console.log(`   🛡️ Mitigación Leonardo: ${risk_assessment.leonardo_risk_mitigation}`);

    // 🚀 RECOMENDACIONES DE EJECUCIÓN
    console.log(`\n🚀 RECOMENDACIONES DE EJECUCIÓN:`);
    const primaryTP = take_profit_levels[0];
    const secondaryTP = take_profit_levels[1];
    const ultimateTP = take_profit_levels.find(tp => tp.type === 'LEONARDO_CONSCIOUSNESS') || take_profit_levels[2];
    
    console.log(`   🎯 ACCIÓN INMEDIATA: ${unified_projection.confidence > 80 ? 'ENTRAR POSICIÓN' : 'ESPERAR'}`);
    console.log(`   💰 LEVERAGE RECOMENDADO: ${leverage_applied}x`);
    console.log(`   🛑 STOP LOSS: $${(current_price * 0.98).toFixed(2)} (-2.0%)`);
    console.log(`   🎯 TAKE PROFIT 1: $${primaryTP.price.toFixed(2)} (${primaryTP.gain_percent})`);
    console.log(`   🎯 TAKE PROFIT 2: $${secondaryTP.price.toFixed(2)} (${secondaryTP.gain_percent})`);
    console.log(`   🌟 ULTIMATE TARGET: $${ultimateTP.price.toFixed(2)} (${ultimateTP.gain_percent})`);
    console.log(`   ⏰ DURACIÓN HOLD: ${unified_projection.expected_timeframe}`);
    console.log(`   🧠 NIVEL CONFIANZA: ${unified_projection.confidence > 85 ? 'MUY ALTO' : unified_projection.confidence > 70 ? 'ALTO' : 'MODERADO'}`);
}

// 🧠 FUNCIÓN PARA MOSTRAR ESTADÍSTICAS DEL SISTEMA
async function showSystemStats() {
    console.log('\n🧠 ===========================================');
    console.log('📊 ESTADÍSTICAS DEL SISTEMA NEURONAL');
    console.log('🧠 ===========================================\n');

    try {
        const response = await axios.get(`${API_BASE_URL}/health`);
        console.log('✅ Sistema Core: CONECTADO');
        console.log('🧠 Proyección Neural: ACTIVA');
        console.log('⚛️ Take Profit Cuántico: OPERATIVO');
        console.log('🎯 Neuronas Especializadas: 7 ACTIVAS');
        console.log('📈 Timeframes Cuánticos: 10 CONFIGURADOS');
        console.log('🎭 Zonas Take Profit: 20+ DINÁMICAS');
    } catch (error) {
        console.log('❌ Sistema Core: DESCONECTADO');
    }
}

// 🚀 EJECUCIÓN PRINCIPAL
async function main() {
    try {
        await showSystemStats();
        await testNeuralProjectionQuantum();
        
        console.log('\n🧠 ===========================================');
        console.log('🎯 SISTEMA DE PROYECCIÓN NEURONAL COMPLETADO');
        console.log('⚛️ TAKE PROFIT CUÁNTICO - OPERATIVO');
        console.log('🚀 REVOLUCIÓN EN PREDICCIÓN DE PRECIOS');
        console.log('🧠 ===========================================\n');
        
    } catch (error) {
        console.error('❌ Error en test:', error.message);
    }
}

// 🚀 EJECUTAR TEST
main();
