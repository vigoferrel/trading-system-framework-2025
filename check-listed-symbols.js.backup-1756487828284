const axios = require('axios');

// Funci√≥n para verificar s√≠mbolos realmente listados en Binance Futures
async function checkListedSymbols() {
    try {
        console.log('üîç Verificando s√≠mbolos realmente listados en Binance Futures...');
        
        // Obtener informaci√≥n de exchange para verificar s√≠mbolos listados
        const exchangeInfoResponse = await axios.get('https://fapi.binance.com/fapi/v1/exchangeInfo');
        
        if (exchangeInfoResponse.data && exchangeInfoResponse.data.symbols) {
            const listedSymbols = [];
            const unlistedSymbols = [];
            
            // Obtener la plantilla actual
            const currentTemplate = require('./valid-symbols-template.js').VALID_BINANCE_FUTURES_SYMBOLS;
            
            console.log(`üìä Verificando ${currentTemplate.length} s√≠mbolos de la plantilla...`);
            
            // Verificar cada s√≠mbolo de la plantilla
            currentTemplate.forEach(symbol => {
                const symbolInfo = exchangeInfoResponse.data.symbols.find(item => item.symbol === symbol);
                
                if (symbolInfo && symbolInfo.status === 'TRADING') {
                    listedSymbols.push(symbol);
                } else {
                    unlistedSymbols.push(symbol);
                }
            });
            
            console.log(`‚úÖ S√≠mbolos listados encontrados: ${listedSymbols.length}`);
            console.log(`‚ùå S√≠mbolos no listados encontrados: ${unlistedSymbols.length}`);
            
            if (unlistedSymbols.length > 0) {
                console.log('\nüö® S√çMBOLOS NO LISTADOS DETECTADOS:');
                unlistedSymbols.forEach(symbol => {
                    console.log(`  - ${symbol}`);
                });
            }
            
            // Crear nueva plantilla con solo s√≠mbolos listados
            const newTemplate = `// PLANTILLA √öNICA DE S√çMBOLOS LISTADOS EN BINANCE FUTURES
// Generado autom√°ticamente: ${new Date().toISOString()}
// Total s√≠mbolos listados: ${listedSymbols.length}
// S√≠mbolos no listados eliminados: ${unlistedSymbols.length}

const VALID_BINANCE_FUTURES_SYMBOLS = [
${listedSymbols.map(symbol => `    '${symbol}'`).join(',\n')}
];

module.exports = { VALID_BINANCE_FUTURES_SYMBOLS };`;
            
            // Guardar nueva plantilla
            const fs = require('fs');
            fs.writeFileSync('valid-symbols-template.js', newTemplate);
            
            console.log('\n‚úÖ Nueva plantilla guardada con solo s√≠mbolos listados');
            console.log(`üìä Total s√≠mbolos listados: ${listedSymbols.length}`);
            
            // Verificar espec√≠ficamente MEMEFIUSDT y XEMUSDT
            console.log('\nüîç Verificaci√≥n espec√≠fica:');
            console.log(`MEMEFIUSDT est√° listado: ${listedSymbols.includes('MEMEFIUSDT')}`);
            console.log(`XEMUSDT est√° listado: ${listedSymbols.includes('XEMUSDT')}`);
            
            // Mostrar algunos ejemplos de s√≠mbolos listados
            console.log('\nüìä Ejemplos de s√≠mbolos listados:');
            listedSymbols.slice(0, 10).forEach(symbol => {
                console.log(`  - ${symbol}`);
            });
            
            return {
                listedSymbols,
                unlistedSymbols,
                totalListed: listedSymbols.length,
                totalUnlisted: unlistedSymbols.length
            };
        }
    } catch (error) {
        console.error('‚ùå Error verificando s√≠mbolos listados:', error.message);
        return null;
    }
}

// Ejecutar verificaci√≥n
checkListedSymbols();
