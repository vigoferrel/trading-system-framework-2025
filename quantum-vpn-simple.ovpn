# 🌙 CONFIGURACIÓN OPENVPN SIMPLIFICADA PARA BINANCE API
# Quantum Trading System - IP Whitelisted

# Configuración del cliente
client
dev tun
proto udp

# Servidor VPN para Binance
remote 192.168.173.160 1862

# Configuración de autenticación
auth-user-pass vpn-config/quantum-vpn-credentials.txt
auth-nocache

# Configuración de cifrado (actualizada para OpenVPN 2.6)
data-ciphers AES-256-GCM:AES-128-GCM
auth SHA256

# Configuración de red
resolv-retry infinite
nobind
persist-key
persist-tun

# Configuración de logs
verb 3
mute 20

# Configuración específica para Binance
route-nopull
route 52.84.0.0 255.255.0.0 net_gateway
route 54.230.0.0 255.255.0.0 net_gateway
route 99.84.0.0 255.255.0.0 net_gateway

# Configuración de DNS
dhcp-option DNS 8.8.8.8
dhcp-option DNS 8.8.4.4

# Configuración de seguridad (verificación simplificada)
remote-cert-tls server
# peer-fingerprint SHA256:00:00:00:00:00:00:00:00:00:00:00:00:00:00:00:00:00:00:00:00:00:00:00:00:00:00:00:00:00:00:00:00
tls-verify false
