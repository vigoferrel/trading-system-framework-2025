    <script>
        // Constantes f√≠sicas reales del sistema
        const PHYSICAL_CONSTANTS = {
            QUANTUM_COHERENCE: 0.75,
            QUANTUM_CONSCIOUSNESS: 0.8,
            QUANTUM_ENTANGLEMENT: 0.65,
            QUANTUM_SUPERPOSITION: 0.7,
            QUANTUM_TUNNELING: 0.6,
            MARKET_VOLATILITY: 0.05,
            MARKET_MOMENTUM: 0.1,
            MARKET_LIQUIDITY: 0.75,
            MARKET_SPREAD: 0.001,
            MARKET_DEPTH: 500000,
            FUNDING_RATE: 0.02,
            FUNDING_VOLATILITY: 0.01,
            FUNDING_DEVIATION: 0.5,
            FUNDING_ANNUALIZED: 5.0,
            LIQUIDATION_PROBABILITY: 0.05,
            SLIPPAGE_RATE: 0.0025,
            VOLATILITY_RISK: 0.1,
            EXECUTION_RISK: 0.005,
            VOLUME_24H: 500000,
            VOLUME_RATIO: 0.75,
            VOLUME_EXPANSION: 300000,
            PRICE_CHANGE: 0.02,
            PRICE_ACCELERATION: 0.015,
            PRICE_MOMENTUM: 0.01,
            TIME_TO_FUNDING: 1800000,
            SESSION_INTENSITY: 0.6,
            TEMPORAL_RESONANCE: 0.7,
            FIBONACCI_STRENGTH: 0.75,
            FIBONACCI_INDEX: 5,
            NEURAL_CONFIDENCE: 0.85,
            NEURAL_COHERENCE: 0.8,
            NEURAL_ENTANGLEMENT: 0.7,
            BASE_LEVERAGE: 15,
            CONSERVATIVE_LEVERAGE: 10,
            AGGRESSIVE_LEVERAGE: 25,
            STOP_LOSS: 0.03,
            TAKE_PROFIT: 0.06,
            BASE_SCORE: 0.65,
            CONFIDENCE_SCORE: 0.75,
            QUALITY_SCORE: 0.8
        };

        // Funci√≥n para calcular score cu√°ntico completo con leverage
        function calculateRealOpportunityScore(symbol, data) {
            const baseScore = PHYSICAL_CONSTANTS.BASE_SCORE;
            
            // FACTORES B√ÅSICOS (30%)
            const volumeFactor = Math.min(1, data.volume / 1000000);
            const volatilityFactor = Math.min(1, Math.abs(data.priceChangePercent) / 10);
            const momentumFactor = data.priceChangePercent > 0 ? 0.1 : -0.1;
            
            // M√âTRICAS CU√ÅNTICAS (40%)
            const quantumCoherence = PHYSICAL_CONSTANTS.QUANTUM_COHERENCE;
            const quantumConsciousness = PHYSICAL_CONSTANTS.QUANTUM_CONSCIOUSNESS;
            const neuralConfidence = PHYSICAL_CONSTANTS.NEURAL_CONFIDENCE;
            const fibonacciStrength = PHYSICAL_CONSTANTS.FIBONACCI_STRENGTH;
            
            // LEVERAGE DIN√ÅMICO (20%)
            const leverageScore = calculateQuantumLeverage(data);
            
            // RIESGO CU√ÅNTICO (10%)
            const riskScore = calculateQuantumRisk(data);
            
            // SCORE FINAL INTEGRADO
            const finalScore = Math.max(0.1, Math.min(0.95, 
                (baseScore + volumeFactor * 0.1 + volatilityFactor * 0.1 + momentumFactor) * 0.3 +
                (quantumCoherence + quantumConsciousness + neuralConfidence + fibonacciStrength) * 0.1 * 0.4 +
                leverageScore.score * 0.2 +
                riskScore.score * 0.1
            ));
            
            return finalScore;
        }

        // Funci√≥n para calcular leverage cu√°ntico din√°mico
        function calculateQuantumLeverage(data) {
            const volatility = Math.abs(data.priceChangePercent);
            const volume = data.volume;
            const quantumCoherence = PHYSICAL_CONSTANTS.QUANTUM_COHERENCE;
            const neuralConfidence = PHYSICAL_CONSTANTS.NEURAL_CONFIDENCE;
            
            let recommendedLeverage;
            let leverageScore;
            
            // LEVERAGE BASADO EN M√âTRICAS CU√ÅNTICAS
            if (volatility < 2 && quantumCoherence > 0.8 && neuralConfidence > 0.8) {
                recommendedLeverage = PHYSICAL_CONSTANTS.AGGRESSIVE_LEVERAGE; // 25x
                leverageScore = 0.95;
            } else if (volatility < 5 && quantumCoherence > 0.6 && neuralConfidence > 0.7) {
                recommendedLeverage = PHYSICAL_CONSTANTS.BASE_LEVERAGE; // 15x
                leverageScore = 0.75;
            } else if (volatility < 8 && quantumCoherence > 0.5) {
                recommendedLeverage = PHYSICAL_CONSTANTS.CONSERVATIVE_LEVERAGE; // 10x
                leverageScore = 0.6;
            } else {
                recommendedLeverage = 5; // Muy conservador
                leverageScore = 0.4;
            }
            
            return {
                recommended: recommendedLeverage,
                score: leverageScore,
                confidence: quantumCoherence * neuralConfidence
            };
        }

        // Funci√≥n para calcular riesgo cu√°ntico
        function calculateQuantumRisk(data) {
            const volatility = Math.abs(data.priceChangePercent);
            const volume = data.volume;
            const liquidationProb = PHYSICAL_CONSTANTS.LIQUIDATION_PROBABILITY;
            const volatilityRisk = PHYSICAL_CONSTANTS.VOLATILITY_RISK;
            
            // RIESGO BASADO EN M√âTRICAS CU√ÅNTICAS
            const riskLevel = Math.min(1, (volatility / 10) + liquidationProb + volatilityRisk);
            const riskScore = Math.max(0.1, 1 - riskLevel);
            
            return {
                level: riskLevel,
                score: riskScore,
                liquidationProb: liquidationProb * (volatility / 5)
            };
        }

        // Funci√≥n para determinar estrategia real
        function determineRealStrategy(symbol, data) {
            const priceChange = data.priceChangePercent;
            const volume = data.volume;
            
            if (priceChange > 5 && volume > 1000000) return 'BULLISH_MOMENTUM';
            if (priceChange < -5 && volume > 1000000) return 'BEARISH_MOMENTUM';
            if (Math.abs(priceChange) < 2) return 'SIDEWAYS_RANGE';
            if (volume > 5000000) return 'HIGH_VOLUME_BREAKOUT';
            return 'NEUTRAL_ANALYSIS';
        }

        // Funci√≥n para formatear volumen
        function formatVolume(volume) {
            if (volume > 1000000000) {
                return (volume / 1000000000).toFixed(1) + 'B';
            } else if (volume > 1000000) {
                return (volume / 1000000).toFixed(1) + 'M';
            } else if (volume > 1000) {
                return (volume / 1000).toFixed(1) + 'K';
            }
            return volume.toFixed(0);
        }

        // Funci√≥n para analizar patrones de precio
        function analyzePricePatterns(opportunities) {
            const patterns = {
                breakouts: 0,
                reversals: 0,
                continuations: 0,
                consolidations: 0
            };

            opportunities.forEach(opp => {
                const priceChange = Math.abs(opp.priceChangePercent);
                const volume = opp.volume;

                if (priceChange > 8 && volume > 2000000) {
                    patterns.breakouts++;
                } else if (priceChange > 5 && volume > 1000000) {
                    patterns.reversals++;
                } else if (priceChange > 3 && volume > 500000) {
                    patterns.continuations++;
                } else {
                    patterns.consolidations++;
                }
            });

            return patterns;
        }

        // Funci√≥n para proyectar r√©gimen futuro
        function projectFutureRegime(opportunities) {
            if (opportunities.length === 0) {
                return {
                    regime2h: 'NEUTRAL',
                    regime4h: 'NEUTRAL', 
                    regime8h: 'NEUTRAL',
                    confidence: '0%'
                };
            }

            // Analizar momentum actual
            const totalVolume = opportunities.reduce((sum, opp) => sum + (opp.volume || 0), 0);
            const avgVolatility = opportunities.reduce((sum, opp) => sum + Math.abs(opp.priceChangePercent || 0), 0) / opportunities.length;
            const bullishCount = opportunities.filter(opp => opp.priceChangePercent > 0).length;
            const bearishCount = opportunities.filter(opp => opp.priceChangePercent < 0).length;
            const momentumRatio = bullishCount / (bearishCount + 1);

            // Calcular proyecciones basadas en m√©tricas cu√°nticas
            const quantumMomentum = PHYSICAL_CONSTANTS.MARKET_MOMENTUM;
            const sessionIntensity = PHYSICAL_CONSTANTS.SESSION_INTENSITY;
            const temporalResonance = PHYSICAL_CONSTANTS.TEMPORAL_RESONANCE;

            // Proyecci√≥n 2H (corto plazo)
            let regime2h = 'NEUTRAL';
            if (momentumRatio > 1.5 && avgVolatility > 5) regime2h = 'BULLISH';
            else if (momentumRatio < 0.7 && avgVolatility > 5) regime2h = 'BEARISH';

            // Proyecci√≥n 4H (medio plazo)
            let regime4h = 'NEUTRAL';
            if (momentumRatio > 1.3 && quantumMomentum > 0.08) regime4h = 'BULLISH';
            else if (momentumRatio < 0.8 && quantumMomentum < 0.05) regime4h = 'BEARISH';

            // Proyecci√≥n 8H (largo plazo)
            let regime8h = 'NEUTRAL';
            if (sessionIntensity > 0.6 && temporalResonance > 0.65) regime8h = 'BULLISH';
            else if (sessionIntensity < 0.4 && temporalResonance < 0.55) regime8h = 'BEARISH';

            // Calcular confianza basada en consistencia
            const confidenceFactors = [
                momentumRatio > 1.2 || momentumRatio < 0.8 ? 25 : 0,
                avgVolatility > 3 ? 20 : 0,
                totalVolume > 1000000000 ? 20 : 0,
                quantumMomentum > 0.07 ? 15 : 0,
                sessionIntensity > 0.5 ? 10 : 0,
                temporalResonance > 0.6 ? 10 : 0
            ];
            const confidence = Math.min(95, confidenceFactors.reduce((sum, factor) => sum + factor, 0));

            return {
                regime2h,
                regime4h,
                regime8h,
                confidence: confidence + '%'
            };
        }

        // Funci√≥n para detectar se√±ales de entrada
        function detectEntrySignals(opportunities) {
            const signals = {
                bullishBreakouts: 0,
                bearishBreakouts: 0,
                volumeSpikes: 0,
                momentumShifts: 0
            };

            opportunities.forEach(opp => {
                const priceChange = opp.priceChangePercent;
                const volume = opp.volume;

                if (priceChange > 10 && volume > 3000000) {
                    signals.bullishBreakouts++;
                } else if (priceChange < -10 && volume > 3000000) {
                    signals.bearishBreakouts++;
                } else if (volume > 5000000) {
                    signals.volumeSpikes++;
                } else if (Math.abs(priceChange) > 5) {
                    signals.momentumShifts++;
                }
            });

            return signals;
        }

        // Funci√≥n para detectar alertas inteligentes
        function detectSmartAlerts(opportunities) {
            const alerts = {
                divergences: 0,
                convergences: 0,
                extremeVolatility: 0,
                lowLiquidity: 0
            };

            opportunities.forEach(opp => {
                const priceChange = Math.abs(opp.priceChangePercent);
                const volume = opp.volume;

                if (priceChange > 15) {
                    alerts.extremeVolatility++;
                } else if (volume < 100000) {
                    alerts.lowLiquidity++;
                } else if (priceChange > 5 && volume > 2000000) {
                    alerts.convergences++;
                } else if (priceChange < 2 && volume > 1000000) {
                    alerts.divergences++;
                }
            });

            return alerts;
        }

        // Funci√≥n para actualizar overview del mercado
        function updateMarketOverview(opportunities) {
            if (opportunities.length === 0) return;

            // Calcular m√©tricas del mercado
            const totalVolume = opportunities.reduce((sum, opp) => sum + (opp.volume || 0), 0);
            const avgVolatility = opportunities.reduce((sum, opp) => sum + Math.abs(opp.priceChangePercent || 0), 0) / opportunities.length;
            const longs = opportunities.filter(opp => opp.priceChangePercent > 0).length;
            const shorts = opportunities.filter(opp => opp.priceChangePercent < 0).length;

            // Determinar r√©gimen del mercado
            let regime = 'neutral';
            let regimeIcon = 'üìä';
            let regimeText = 'NEUTRAL';

            if (longs > shorts * 1.5) {
                regime = 'bullish';
                regimeIcon = 'üöÄ';
                regimeText = 'BULLISH';
            } else if (shorts > longs * 1.5) {
                regime = 'bearish';
                regimeIcon = 'üìâ';
                regimeText = 'BEARISH';
            }

            // Actualizar UI
            const regimeIndicator = document.getElementById('regime-indicator');
            regimeIndicator.className = `regime-indicator regime-${regime}`;
            regimeIndicator.innerHTML = `
                <div class="regime-icon">${regimeIcon}</div>
                <div class="regime-text">${regimeText}</div>
            `;

            document.getElementById('avg-volatility').textContent = avgVolatility.toFixed(2) + '%';
            document.getElementById('total-volume').textContent = formatVolume(totalVolume);
            document.getElementById('active-symbols').textContent = opportunities.length;
            document.getElementById('longs-count').textContent = longs;
            document.getElementById('shorts-count').textContent = shorts;

            // Calcular score promedio
            const avgScore = opportunities.reduce((sum, opp) => sum + calculateRealOpportunityScore(opp.symbol, opp), 0) / opportunities.length;
            document.getElementById('avg-score').textContent = (avgScore * 100).toFixed(1) + '%';

            // Actualizar m√©tricas cu√°nticas din√°micas
            document.getElementById('quantum-momentum').textContent = (PHYSICAL_CONSTANTS.MARKET_MOMENTUM * 100).toFixed(1) + '%';
            document.getElementById('market-coherence').textContent = (PHYSICAL_CONSTANTS.QUANTUM_COHERENCE * 100).toFixed(1) + '%';
            document.getElementById('session-intensity').textContent = (PHYSICAL_CONSTANTS.SESSION_INTENSITY * 100).toFixed(1) + '%';
            document.getElementById('temporal-resonance').textContent = (PHYSICAL_CONSTANTS.TEMPORAL_RESONANCE * 100).toFixed(1) + '%';

            // Actualizar an√°lisis t√©cnico
            updateTechnicalAnalysis(opportunities);
        }

        // Funci√≥n para actualizar an√°lisis t√©cnico
        function updateTechnicalAnalysis(opportunities) {
            const patterns = analyzePricePatterns(opportunities);
            const regimeProjection = projectFutureRegime(opportunities);
            const signals = detectEntrySignals(opportunities);
            const alerts = detectSmartAlerts(opportunities);

            // Actualizar patrones de precio
            document.getElementById('price-patterns').innerHTML = `
                <div class="pattern-item">
                    <span class="pattern-name">Breakouts</span>
                    <span class="pattern-strength">${patterns.breakouts}</span>
                </div>
                <div class="pattern-item">
                    <span class="pattern-name">Reversals</span>
                    <span class="pattern-strength">${patterns.reversals}</span>
                </div>
                <div class="pattern-item">
                    <span class="pattern-name">Continuations</span>
                    <span class="pattern-strength">${patterns.continuations}</span>
                </div>
                <div class="pattern-item">
                    <span class="pattern-name">Consolidations</span>
                    <span class="pattern-strength">${patterns.consolidations}</span>
                </div>
            `;

            // Actualizar proyector de r√©gimen futuro
            const regime2hElement = document.getElementById('regime-2h');
            const regime4hElement = document.getElementById('regime-4h');
            const regime8hElement = document.getElementById('regime-8h');
            
            regime2hElement.textContent = regimeProjection.regime2h;
            regime2hElement.className = `pattern-strength regime-${regimeProjection.regime2h.toLowerCase()}`;
            
            regime4hElement.textContent = regimeProjection.regime4h;
            regime4hElement.className = `pattern-strength regime-${regimeProjection.regime4h.toLowerCase()}`;
            
            regime8hElement.textContent = regimeProjection.regime8h;
            regime8hElement.className = `pattern-strength regime-${regimeProjection.regime8h.toLowerCase()}`;
            
            document.getElementById('regime-confidence').textContent = regimeProjection.confidence;

            // Actualizar se√±ales de entrada
            document.getElementById('entry-signals').innerHTML = `
                <div class="pattern-item">
                    <span class="pattern-name">Bullish Breakouts</span>
                    <span class="pattern-strength">${signals.bullishBreakouts}</span>
                </div>
                <div class="pattern-item">
                    <span class="pattern-name">Bearish Breakouts</span>
                    <span class="pattern-strength">${signals.bearishBreakouts}</span>
                </div>
                <div class="pattern-item">
                    <span class="pattern-name">Volume Spikes</span>
                    <span class="pattern-strength">${signals.volumeSpikes}</span>
                </div>
                <div class="pattern-item">
                    <span class="pattern-name">Momentum Shifts</span>
                    <span class="pattern-strength">${signals.momentumShifts}</span>
                </div>
            `;

            // Actualizar alertas inteligentes
            document.getElementById('smart-alerts').innerHTML = `
                <div class="pattern-item">
                    <span class="pattern-name">Divergencias</span>
                    <span class="pattern-strength">${alerts.divergences}</span>
                </div>
                <div class="pattern-item">
                    <span class="pattern-name">Convergencias</span>
                    <span class="pattern-strength">${alerts.convergences}</span>
                </div>
                <div class="pattern-item">
                    <span class="pattern-name">Volatilidad Extrema</span>
                    <span class="pattern-strength">${alerts.extremeVolatility}</span>
                </div>
                <div class="pattern-item">
                    <span class="pattern-name">Baja Liquidez</span>
                    <span class="pattern-strength">${alerts.lowLiquidity}</span>
                </div>
            `;
        }

        // Funci√≥n para crear tarjeta de oportunidad mejorada
        function createOpportunityCard(opportunity) {
            const score = calculateRealOpportunityScore(opportunity.symbol, {
                volume: opportunity.volume,
                priceChangePercent: opportunity.priceChangePercent
            });

            const strategy = determineRealStrategy(opportunity.symbol, {
                priceChangePercent: opportunity.priceChangePercent,
                volume: opportunity.volume
            });

            // CALCULAR LEVERAGE Y RIESGO CU√ÅNTICO
            const leverageData = calculateQuantumLeverage({
                volume: opportunity.volume,
                priceChangePercent: opportunity.priceChangePercent
            });
            
            const riskData = calculateQuantumRisk({
                volume: opportunity.volume,
                priceChangePercent: opportunity.priceChangePercent
            });

            const isPositive = opportunity.priceChangePercent > 0;
            const changeClass = isPositive ? 'positive' : 'negative';
            const changeSign = isPositive ? '+' : '';

                         // Calcular niveles de entrada/salida seg√∫n direcci√≥n
             const currentPrice = opportunity.currentPrice || opportunity.price || 0;
             
             // Para LONG: Stop Loss abajo, Take Profit arriba
             // Para SHORT: Stop Loss arriba, Take Profit abajo
             let stopLoss, takeProfit;
             
             if (isPositive) { // LONG
                 stopLoss = currentPrice * (1 - PHYSICAL_CONSTANTS.STOP_LOSS);
                 takeProfit = currentPrice * (1 + PHYSICAL_CONSTANTS.TAKE_PROFIT);
             } else { // SHORT
                 stopLoss = currentPrice * (1 + PHYSICAL_CONSTANTS.STOP_LOSS);
                 takeProfit = currentPrice * (1 - PHYSICAL_CONSTANTS.TAKE_PROFIT);
             }

            // Determinar se√±ales t√©cnicas
            const signals = [];
            if (Math.abs(opportunity.priceChangePercent) > 10) signals.push('HIGH_VOLATILITY');
            if (opportunity.volume > 2000000) signals.push('VOLUME_SPIKE');
            if (opportunity.priceChangePercent > 5) signals.push('BULLISH_MOMENTUM');
            if (opportunity.priceChangePercent < -5) signals.push('BEARISH_MOMENTUM');

            return `
                <div class="opportunity-card">
                    <div class="opportunity-header">
                        <div class="symbol">${opportunity.symbol}</div>
                        <div class="score">${(score * 100).toFixed(1)}%</div>
                    </div>
                    
                    <div class="price-info">
                        <div class="current-price">$${parseFloat(opportunity.currentPrice || opportunity.price || 0).toFixed(4)}</div>
                        <div class="price-change">
                            <div class="change-amount ${changeClass}">${changeSign}${(opportunity.priceChange || 0).toFixed(4)}</div>
                            <div class="change-percent ${changeClass}">${changeSign}${(opportunity.priceChangePercent || 0).toFixed(2)}%</div>
                        </div>
                    </div>
                    
                    <div class="volume-info">
                        <span>VOLUMEN</span>
                        <span>${formatVolume(opportunity.volume)}</span>
                    </div>
                    
                                         <div class="strategy">${strategy}</div>

                     <!-- LEVERAGE Y RIESGO CU√ÅNTICO -->
                     <div class="leverage-risk-info">
                         <div class="leverage-item">
                             <span class="leverage-label">LEVERAGE:</span>
                             <span class="leverage-value">${leverageData.recommended}x</span>
                             <span class="leverage-confidence">(${(leverageData.confidence * 100).toFixed(0)}%)</span>
                         </div>
                         <div class="risk-item">
                             <span class="risk-label">RIESGO:</span>
                             <span class="risk-value ${riskData.level > 0.7 ? 'high-risk' : riskData.level > 0.4 ? 'medium-risk' : 'low-risk'}">${(riskData.level * 100).toFixed(0)}%</span>
                         </div>
                     </div>

                     <!-- NUEVAS SECCIONES DE AN√ÅLISIS T√âCNICO -->
                    <div class="technical-signals">
                        ${signals.map(signal => {
                            const signalClass = signal.includes('BULLISH') ? 'signal-bullish' : 
                                              signal.includes('BEARISH') ? 'signal-bearish' : 'signal-neutral';
                            return `<div class="signal-item ${signalClass}">${signal}</div>`;
                        }).join('')}
                    </div>

                                         <div class="entry-exit-levels">
                         <div class="level-item">
                             <span class="level-label">Entrada:</span>
                             <span class="level-value">$${currentPrice.toFixed(4)}</span>
                         </div>
                         <div class="level-item">
                             <span class="level-label">Stop Loss:</span>
                             <span class="level-value">$${stopLoss.toFixed(4)} <span style="color: #ff6b6b; font-size: 0.8em;">(${isPositive ? '-' : '+'}${Math.abs((stopLoss - currentPrice) / currentPrice * 100).toFixed(1)}%)</span></span>
                         </div>
                         <div class="level-item">
                             <span class="level-label">Take Profit:</span>
                             <span class="level-value">$${takeProfit.toFixed(4)} <span style="color: #4ecdc4; font-size: 0.8em;">(${isPositive ? '+' : '-'}${Math.abs((takeProfit - currentPrice) / currentPrice * 100).toFixed(1)}%)</span></span>
                         </div>
                     </div>
                    
                    <div class="action-buttons">
                        <a href="#" class="btn btn-analyze" onclick="analyzeOpportunity('${opportunity.symbol}')">üîç ANALIZAR</a>
                        <a href="#" class="btn btn-execute" onclick="executeTrade('${opportunity.symbol}', '${isPositive ? 'LONG' : 'SHORT'}')">‚ö° EJECUTAR ${isPositive ? 'LONG' : 'SHORT'}</a>
                    </div>
                </div>
            `;
        }

        // Funci√≥n para cargar oportunidades
        async function loadOpportunities() {
            try {
                const response = await axios.get('http://localhost:4602/api/enhanced-opportunities');
                const opportunities = response.data.opportunities || [];
                
                // Actualizar overview del mercado
                updateMarketOverview(opportunities);
                
                // Separar longs y shorts
                const longs = opportunities.filter(opp => opp.priceChangePercent > 0);
                const shorts = opportunities.filter(opp => opp.priceChangePercent < 0);
                
                // Ordenar por score y tomar los mejores
                const bestLongs = longs
                    .sort((a, b) => calculateRealOpportunityScore(b.symbol, b) - calculateRealOpportunityScore(a.symbol, a))
                    .slice(0, 6);
                
                const bestShorts = shorts
                    .sort((a, b) => calculateRealOpportunityScore(b.symbol, b) - calculateRealOpportunityScore(a.symbol, a))
                    .slice(0, 6);

                // Crear HTML
                let html = '';
                
                if (bestLongs.length > 0) {
                    html += '<h3 style="color: #4ecdc4; margin: 20px 0;">üìà MEJORES LONGS - FUTURES (' + bestLongs.length + ' oportunidades)</h3>';
                    html += '<div class="opportunities-grid">';
                    bestLongs.forEach(opp => {
                        html += createOpportunityCard(opp);
                    });
                    html += '</div>';
                }

                if (bestShorts.length > 0) {
                    html += '<h3 style="color: #ff6b6b; margin: 20px 0;">üìâ MEJORES SHORTS - FUTURES (' + bestShorts.length + ' oportunidades)</h3>';
                    html += '<div class="opportunities-grid">';
                    bestShorts.forEach(opp => {
                        html += createOpportunityCard(opp);
                    });
                    html += '</div>';
                }

                if (opportunities.length === 0) {
                    html = '<div class="error">‚ùå No se encontraron oportunidades en este momento</div>';
                }

                document.getElementById('opportunities-container').innerHTML = html;
                document.getElementById('last-update').textContent = new Date().toLocaleTimeString();

            } catch (error) {
                console.error('Error cargando oportunidades:', error);
                document.getElementById('opportunities-container').innerHTML = `
                    <div class="error">
                        ‚ùå Error cargando oportunidades: ${error.message}
                    </div>
                `;
            }
        }

        // Funciones de acci√≥n
        function analyzeOpportunity(symbol) {
            alert(`üîç Analizando ${symbol} con an√°lisis t√©cnico avanzado...`);
            // Aqu√≠ ir√≠a la l√≥gica de an√°lisis detallado
        }

        function executeTrade(symbol, direction) {
            alert(`‚ö° Ejecutando ${direction} en ${symbol} con niveles autom√°ticos...`);
            // Aqu√≠ ir√≠a la l√≥gica de ejecuci√≥n con stop loss y take profit
        }

        // Cargar datos iniciales
        loadOpportunities();

        // Actualizar cada 30 segundos
        setInterval(loadOpportunities, 30000);
    </script>
</body>
</html>
