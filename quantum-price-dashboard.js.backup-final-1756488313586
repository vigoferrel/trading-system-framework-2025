        }
        
        .bullish { border-color: #00ff00; background: #002200; }
        .bearish { border-color: #ff0000; background: #220000; }
        .neutral { border-color: #ffff00; background: #222200; }
        
        .refresh-btn {
            background: #00ff00;
            color: #000;
            border: none;
